<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Photographer Dashboard</title>
</head>
<body>
    <h1>Photographer Dashboard</h1>
    <div class="excel-board">
        <!-- Only show events assigned to the photographer -->
    </div>
    <p>Assigned Events:</p>
    <ul>
        <% assignedEvents.forEach(event => { %>
            <li><%= event %></li>
        <% }) %>
    </ul>
</body>
</html>
